# Application logs
[WinEventLog://Application]
index = endpoint
disabled = false


# Security event logs
[WinEventLog://Security]
index = endpoint
disabled = false

# System logs
[WinEventLog://System]
index = endpoint
disabled = false

# Enable Sysmon logs
[WinEventLog://Microsoft-Windows-Sysmon/Operational]
index = endpoint
disabled = false
renderXml = true
source = XmlWinEventLog://Microsoft-Windows-Sysmon/Operational





# Application logs
[WinEventLog://Application]
disabled = 0
start_from = oldest
current_only = 0
